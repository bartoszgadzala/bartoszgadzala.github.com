---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Bartosz Gadzała</title>
 <link href="http://bartoszgadzala.github.com/atom.xml" rel="self"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://bartoszgadzala.github.com/</id>
 <author>
   <name>Bartosz Gadzała</name>
   <email>bartosz.gadzala@gmail.com</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://bartosz.gadzala.github.com/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://bartoszgadzala.github.com/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
